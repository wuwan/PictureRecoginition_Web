<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>AddRecordServlet</servlet-name>
    <servlet-class>servlet.AddRecordServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ImagePushServlet</servlet-name>
    <servlet-class>servlet.ImagePushServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QueryClassifyServlet</servlet-name>
    <servlet-class>servlet.QueryClassifyServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ImportImageServlet</servlet-name>
    <servlet-class>servlet.ImportImageServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>AddClassifyServlet</servlet-name>
    <servlet-class>servlet.AddClassifyServlet</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>AddRecordServlet</servlet-name>
    <url-pattern>/servlet/AddRecordServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ImagePushServlet</servlet-name>
    <url-pattern>/servlet/ImagePushServlet</url-pattern>
  </servlet-mapping>	
  <servlet-mapping>
    <servlet-name>QueryClassifyServlet</servlet-name>
    <url-pattern>/servlet/QueryClassifyServlet</url-pattern>
  </servlet-mapping>	
  <servlet-mapping>
    <servlet-name>ImportImageServlet</servlet-name>
    <url-pattern>/servlet/ImportImageServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddClassifyServlet</servlet-name>
    <url-pattern>/servlet/AddClassifyServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
